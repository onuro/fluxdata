{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "fluxdata/fluxinfo",
	"version": "0.1.0",
	"title": "Flux Info",
	"category": "widgets",
	"icon": "chart-area",
	"description": "Display Flux network statistics including node count and running applications.",
	"example": {},
	"attributes": {
		"dataType": {
			"type": "string",
			"default": "nodecount"
		},
		"humanReadable": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"color": {
			"heading": true,
			"gradients": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"textdomain": "fluxdata",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
